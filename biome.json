{
  "$schema": "https://biomejs.dev/schemas/1.8.3/schema.json",
  "files": {
		"ignoreUnknown": false,
		"ignore": [
			"dist",
			".turbo",
			".expo",
      "**/*.css"
		]
	},
  "organizeImports": {
    "enabled": true
  },
  "linter": {
    "enabled": true,
    "rules": {
      "recommended": true,
      "suspicious": {
        "noExplicitAny": "off"
      }
    }
  }
}
